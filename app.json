      "package": "com.growme.app"
    },
    "plugins": [
      "expo-font",
      "expo-web-browser",
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "76b3fff5-bcb6-4f36-a188-1526ac42fa8e"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
